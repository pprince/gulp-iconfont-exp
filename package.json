{
  "name": "gulp-starter",
  "version": "0.1.0",
  "description": "Gulp starter with common tasks and scenarios",
  "repository": {
    "type": "git",
    "url": "git://github.com/greypants/gulp-starter.git"
  },
  "browser": {
    "plugin": "./src/javascript/vendor/jquery-plugin.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify",
      "hbsfy"
    ]
  },
  "browserify-shim": {
    "plugin": {
      "exports": "plugin",
      "depends": [
        "jquery:$"
      ]
    }
  },
  "devDependencies": {
    "browser-sync": "~0.8.2",
    "browserify-shim": "~3.4.1",
    "browserify": "~3.44.2",
    "coffeeify": "~0.6.0",
    "gulp-changed": "~0.3.0",
    "gulp-compass": "~1.1.8",
    "gulp-imagemin": "~0.3.0",
    "gulp-notify": "~1.2.5",
    "gulp-open": "~0.2.8",
    "gulp": "~3.6.2",
    "hbsfy": "~1.3.2",
    "vinyl-source-stream": "~0.1.1",
    "gulp-iconfont": "pprince/gulp-iconfont#log-and-error-options",
    "gulp-consolidate": "~0.1.2",
    "gulp-util": "~2.2.14",
    "swig": "~1.3.2"
  },
  "dependencies": {
    "backbone": "~1.1.2",
    "jquery": "~2.1.0"
  }
}
